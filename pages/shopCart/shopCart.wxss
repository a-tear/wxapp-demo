page {
    background-color: #f2f2f2;
    min-height: 100%;
}

.tips {
    padding-top: 200rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.tips .icon {
    margin: 0 auto;
    width: 100rpx;
    height: 100rpx;
    margin-bottom: 50rpx;
}

.icon image {
    width: 100%;
    height: 100%;
}

.text {
    margin-bottom: 50rpx;
    color: #999;
    font-size: 28rpx;
    text-align: center;
}

.default {
    background-color: #e64340;
    color: #fff;
    font-size: 28rpx;
}